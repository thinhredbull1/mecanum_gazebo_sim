<?xml version="1.0"?>
<launch>
<!-- <arg name="x_pos" default="7.69" />
    <arg name="y_pos" default="8.74" />
    <arg name="z_pos" default="0.00108" />
    <arg name="yaw" default="3.10" /> -->
    <arg name="x_pos" default="0" />
    <arg name="y_pos" default="0" />
    <arg name="z_pos" default="0.3" />
    <arg name="yaw" default="-1.57079" />
    <arg name="use_sim_time" default="true" />
    <arg name="gui" default="true" />
    <arg name="headless" default="false" />
  <!-- <arg name="world_name" default="$(find nexus_gazebo)/worlds/nexus_4wd_mecanum.world" /> -->
  <node pkg="tf" type="static_transform_publisher" name="map_to_base_link" args="0 0 0 0 0.0 0 odom base_footprint 1000" />
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <!-- <arg name="world_name" value="$(find turtlebot3_gazebo)/worlds/robocon.world"/> -->
   <!-- <arg name="world_name" value="$(find turtlebot3_gazebo)/worlds/turtlebot3_ar.world"/> -->
    <!-- <arg name="world_name" value="$(find aruco_navigation)/worlds/bot_world.world"/>  -->
    <arg name="debug" value="0" />
    <arg name="gui" value="$(arg gui)" />
    <arg name="use_sim_time" value="$(arg use_sim_time)" />
    <arg name="headless" value="$(arg headless)" />
    <!-- <arg name="world_name" value="$(arg world_name)" /> -->
    <arg name="paused" value="false"/>
  </include>

<!-- Load robot description -->
<include file="$(find nexus_4wd_mecanum_description)/launch/nexus_4wd_mecanum_description.launch" />

  <!-- Spawn the robot -->
<node name="urdf_spawner" pkg="gazebo_ros" type="spawn_model"
        args="-urdf -model nexus_4wd_mecanum -param robot_description -x $(arg x_pos) -y $(arg y_pos) -z $(arg z_pos) -Y $(arg yaw)" />

</launch>
